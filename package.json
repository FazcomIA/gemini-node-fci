  "keywords": [
    "n8n-community-node-package",
    "google-gemini",
    "ai",
    "n8n",
    "automation",
    "fci"
  ],
  "license": "MIT",
  "author": {
    "name": "Mateus Gomes",
    "email": "mateusgomes@example.com"
  },
  "repository": {
    "type": "git",
